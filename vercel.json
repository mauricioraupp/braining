{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/css/*",
      "use": "@vercel/static"
    },
    {
      "src": "pages/js/*",
      "use": "@vercel/static"
    },
    {
      "src": "games/memoryGame/html/*",
      "use": "@vercel/static"
    },
    {
      "src": "games/memoryGame/css/*",
      "use": "@vercel/static"
    },
    {
      "src": "games/memoryGame/js/memoryGame.js",
      "use": "@vercel/static"
    },
    {
      "src": "games/puzzleGame/html/*",
      "use": "@vercel/static"
    },
    {
      "src": "games/puzzleGame/css/*",
      "use": "@vercel/static"
    },
    {
      "src": "games/puzzleGame/js/puzzleGame.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/pages/css/(.*)",
      "dest": "/pages/css/$1"
    },
    {
      "src": "/pages/js/(.*)",
      "dest": "/pages/js/$1"
    },
    {
      "src": "/games/memoryGame/images/(.*)",
      "dest": "/games/memoryGame/images/$1"
    },
    {
      "src": "/games/memoryGame/css/(.*)",
      "dest": "/games/memoryGame/css/$1"
    },
    {
      "src": "/games/memoryGame/js/(.*)",
      "dest": "/games/memoryGame/js/$1"
    }
  ]
}
