{
  "version": 2,
  "routes": [
    {
      "src": "^/pages/assets/(.*)$",
      "dest": "/pages/assets/$1"
    },
    {
      "src": "^/assets/(.*)$",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
